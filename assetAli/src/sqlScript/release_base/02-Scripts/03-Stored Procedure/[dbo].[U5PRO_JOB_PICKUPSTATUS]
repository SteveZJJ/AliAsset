USE [EAM];
GO
SET ANSI_NULLS ON;
GO
SET QUOTED_IDENTIFIER ON;
GO

ALTER PROCEDURE [dbo].[U5PRO_JOB_PICKUPSTATUS]
WITH EXEC AS CALLER
AS
Begin
  update R5EVENTS
  set evt_status='TS40'
  WHERE EVT_STATUS='TS10'
  AND EVT_UDFCHAR03=N'已邀约待报到'
  AND EVT_JOBTYPE='20RZ'
  AND CONVERT (VARCHAR, EVT_UDFDATE02, 23)
  =CONVERT (VARCHAR, GETDATE (), 23);
  
end ;

GO